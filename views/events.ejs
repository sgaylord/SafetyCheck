<!-- views/profile.ejs -->
<!doctype html>
<html>
<head>
    <% include includes/head %>
</head>

<body class="container">

    <header>
        <% include includes/header %>
    </header>

<div class="container">

    <div class="page-header text-center">
        <h1><span class="fa fa-anchor"></span> Events Page</h1>
        <a href="/logout" class="btn btn-default btn-sm">Logout</a>
    </div>

    <div class="row">

        <!-- LOCAL INFORMATION -->
        <div class="col-sm-6">
            <div class="well">
                <h3><span class="fa fa-user"></span> Local</h3>

                    <p>
                        <strong>id</strong>: <%= Event._id %><br>
                        <strong>Event Name</strong>: <%= Event.EventName %><br>
                        <strong>Event Date</strong>: <%= Event.EventDate %>
                        <strong>Event Description</strong>: <%= Event.EventDescription %>
                    </p>

            </div>
        </div>

    </div>

</div>

    <footer>
        <% include includes/footer %>
    </footer>

</body>
</html>